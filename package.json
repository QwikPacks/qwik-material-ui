{
  "name": "@esthersoftwaredev/qwik-material-ui",
  "version": "0.1.0",
  "description": "A Qwik-native Material Design component library using SCSS",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly --outDir dist",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.ssr.tsx",
    "build.types": "tsc --emitDeclarationOnly --outDir dist",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "keywords": ["qwik", "material-design", "ui-components", "scss"],
  "author": "Esther White",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QwikPacks/qwik-material-ui.git"
  },
  "bugs": {
    "url": "https://github.com/QwikPacks/qwik-material-ui/issues"
  },
  "homepage": "https://github.com/QwikPacks/qwik-material-ui#readme",
  "peerDependencies": {
    "@builder.io/qwik": "^1.14.1"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.14.1",
    "@types/node": "20.14.11",
    "@eslint/js": "latest",
    "eslint": "9.25.1",
    "eslint-plugin-qwik": "^1.14.1",
    "globals": "16.0.0",
    "prettier": "3.3.3",
    "sass": "^1.89.0",
    "typescript": "5.4.5",
    "typescript-eslint": "8.26.1",
    "vite": "5.3.5",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
